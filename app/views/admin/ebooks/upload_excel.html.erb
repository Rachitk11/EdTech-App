<h2>Upload Excel File</h2>
<%= form_for :ebook, url: import_excel_admin_ebooks_path, method: :post, html: { multipart: true } do |f| %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.label :excel_file, 'Excel File' %>
  <%= f.file_field :excel_file %>
  <%= f.submit 'Upload Excel' %>
<% end %>